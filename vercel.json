{
  "version": 2,
  "builds": [
    { "src": "public/", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "public": "/(.*)", "dest": "/index.html" },
    {
      "src": "/js/(.*)",
      "dest": "/src/cliente/"
    },    {
      "src": "/js/(.*)",
      "dest": "/src/produto/"
    },
           {
      "src": "/js/(.*)",
      "dest": "/src/service/"
    },
           {
      "src": "/js/(.*)",
      "dest": "/src/validacoes/"
    }
  ]
}
